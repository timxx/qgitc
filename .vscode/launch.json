{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "QGitc",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Blame",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "args": [
                "blame",
                "${workspaceRoot}/qgitc/main.py"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Commit",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "console": "integratedTerminal",
            "args": [
                "commit"
            ]
        },
        {
            "name": "Branch Compare",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "console": "integratedTerminal",
            "args": [
                "bcompare"
            ]
        },
        {
            "name": "Chat",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "console": "integratedTerminal",
            "args": [
                "chat"
            ]
        },
        {
            "name": "MergeTool",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "args": [
                "mergetool"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Register Shell",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "args": [
                "shell",
                "register"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Unregister Shell",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceRoot}/qgitc.py",
            "args": [
                "shell",
                "unregister"
            ],
            "console": "integratedTerminal"
        },
    ]
}